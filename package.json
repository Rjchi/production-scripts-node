{
  "name": "production-scripts-node",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "node src/index.js",
    "clean": "rmdir /s /q dist",
    "build": "mkdir dist && xcopy /s /y /i src dist",
    "start": "node dist\\index.js",
    "start:prod": "cross-env NODE_ENV=production node dist\\index.js"
  },
  "keywords": [
    "nodejs",
    "express",
    "javascript",
    "nodemon"
  ],
  "author": "Rjchi",
  "license": "ISC",
  "description": "Este repositorio sirve como guía para generar un package.json compatible para producción.",
  "dependencies": {
    "express": "^4.21.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.9"
  },
  "engines": {
    "node": "22.14.0",
    "npm": "10.9.2"
  }
}
